{
  "version": 2,
  "builds": [
    {
      "src": "foodapi/public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "foodapi/public/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "foodapi/public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "foodapi/public/styles.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "foodapi/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "foodapi/public/$1"
    }
  ]
}
